  <br/><%= image_tag "https://maps.googleapis.com/maps/api/staticmap?center=#{location.latitude},#{location.longitude}&size=500x500&zoom=17&key=#{Rails.application.credentials.name_you_choose}" %>

  <br/><%= "#{Rails.application.credentials.name_you_choose}" %>

  <hr/>

<% end %>
